{
    "require": {
        "php":          ">=5.5.9",
        "phpunit/phpunit": "^5.1",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Demo\\UrbanDictionary\\": "src"
        },
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Demo\\Test\\": "tests"
        },
        "classmap": ["tests/"]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
